  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "request": "^2.88.2",
    "sqlite3-simple-api": "0.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.10",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
